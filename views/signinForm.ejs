    <div class="container my-4"
        style="height: 50vh; display: flex; justify-content: center; align-items: center; flex-direction: column; gap: 3rem;">
        <h2>Log In</h2>
        <form class="row g-3" action="/signinrequest" method="post">
            <div class="col-md-12">
                <label for="inputEmail4" class="form-label">Enter email here*</label>
                <input type="email" name="email" class="form-control" id="inputEmail4" autocomplete="off" required>
            </div>
            <div class="col-md-12">
                <label for="inputPassword4" class="form-label">Enter your password*</label>
                <input type="password" name="password" class="form-control" id="inputPassword4" required>
            </div>
            <div class="col-md-12">
                <button class="btn btn-dark" type="submit">LogIn</button>
            </div>
        </form>
    </div>

